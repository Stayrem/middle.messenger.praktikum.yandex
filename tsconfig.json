{
  "compilerOptions": {
    "target": "es2016",
    "lib": [
      "dom",
      "ESNext"
    ],
    "module": "commonjs",
    "sourceMap": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true
  }
}